<h1>Listing taggings</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Question</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @taggings.each do |tagging| %>
  <tr>
    <td><%= tagging.tag_id %></td>
    <td><%= tagging.question_id %></td>
    <td><%= link_to 'Show', tagging %></td>
    <td><%= link_to 'Edit', edit_tagging_path(tagging) %></td>
    <td><%= link_to 'Destroy', tagging, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tagging', new_tagging_path %>
